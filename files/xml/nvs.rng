						<rng:ref name="nvs_model.global"/>
					</choice>
				</zeroOrMore>
				<group>
					<optional>
						<choice>
							<group>
								<group>
									<rng:ref name="nvs_model.div1Like"/>
								</group>
								<zeroOrMore>
									<choice>
										<rng:ref name="nvs_model.frontPart"/>
										<rng:ref name="nvs_model.div1Like"/>
										<rng:ref name="nvs_model.global"/>
									</choice>
								</zeroOrMore>
							</group>
							<group>
								<group>
									<rng:ref name="nvs_model.divLike"/>
								</group>
